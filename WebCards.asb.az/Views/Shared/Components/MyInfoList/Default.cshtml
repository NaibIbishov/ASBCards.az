@model IEnumerable<DTO.DTOEntity.UserDTO>

<div class="dropdown profile-element">
   @foreach (var item in Model)
    {
        <img alt="image" class="rounded-circle" src="~/Webimages/@item.ImageURL" style="width:50px; height: 50px;" />
        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
            <span class="block m-t-xs font-bold">@item.UserName</span>
            <span class="text-muted text-xs block">Əlavə <b class="caret"></b></span>
        </a>
    }
    <ul class="dropdown-menu animated fadeInRight m-t-xs">
        <li><a class="dropdown-item" asp-controller="Login" asp-action="LogOut">Çıxış</a></li>
    </ul>
</div>