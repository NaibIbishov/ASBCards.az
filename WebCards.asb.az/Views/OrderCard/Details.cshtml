@model IEnumerable<DTO.DTOEntity.OrderCardDTO>



@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox ">
                <div class="ibox-content">
                    <a href="#" class="btn btn-success">Excel Import</a>
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th>Ad</th>
                                    <th>Soyad</th>
                                    <th>Ata adı</th>
                                    <th>Seriya</th>
                                    <th>Seriya№</th>
                                    <th>Verilmə yeri</th>
                                    <th>Verilmə tarixi</th>
                                    <th>Fincode</th>
                                    <th>Tel№</th>
                                    <th>Doğum tarixi</th>
                                    <th>Doğulduğu yer</th>
                                    <th>Faktiki ünvan</th>
                                    <th>Ana adı</th>
                                    <th>Qızlıq soyadı</th>
                                    <th>Filial kodu</th>
                                    <th>Hesab No</th>
                                    <th>Sub hesab</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr class="gradeX">
                                        <td>@item.Name</td>
                                        <td>@item.Surname</td>
                                        <td>@item.FatherName</td>
                                        <td>@item.Seriya</td>
                                        <td>@item.SeriyaNo</td>
                                        <td>@item.VerilmeYeri</td>
                                        <td>@item.Verilmetarixi</td>
                                        <td>@item.Fincode.ToUpper()</td>
                                        <td>@item.TelNumber</td>
                                        <td>@item.Dogumtarixi</td>
                                        <td>@item.DogumYeri</td>
                                        <td>@item.FatikiUnvan</td>
                                        <td>@item.MotherName</td>
                                        <td>@item.MotherSurname</td>
                                        <td>@item.FillialKodu</td>
                                        <td>@item.HesabNo</td>
                                        <td>@item.SubHesab</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



